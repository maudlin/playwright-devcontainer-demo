{
  "name": "playwright-devcontainer-demo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "record": "playwright codegen",
    "manual:smart": "playwright test --reporter=list,./manual-csv-reporter.cjs && node scripts/smart-manual-generator.mjs test-results/manual-tests.csv",
    "csv": "playwright test --reporter=list,./manual-csv-reporter.cjs"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^24.3.0",
    "csv-parse": "^5.5.6",
    "csv-stringify": "^6.5.2",
    "typescript": "^5.5.4"
  }
}
